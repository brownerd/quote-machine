*
  box-sizing border-box
  
@import url("http://fonts.googleapis.com/css?family=Roboto\+Slab:300,100,400,700")


 // simple breakpoints
 breakpoints = {
  sm: "(max-width: 850px)",
  med: "(max-width: 1024px)",
  lg: "(max-width: 1600px)"
}

breakpoint(name) 
  @media breakpoints[name]
    {block}

// Flexbox debug tip from Wes Bos
debug(color=red)
  border 1px solid color
  
// Mixins for making the sky, ground and faces of the haus
split(ai=flex-end)
  display flex
  align-items ai
  justify-content center
  height 50vh
  width 100vw
  
haus()
  display flex
  height 90px
  width 120px
  flex-direction column
  
roof()
  display flex
  height 40px
  width 140px
  
roof__side(skew)
  height 40px
  transform skew(skew) translateX(-14px)
  order 2
  flex 2
  
roof__front(skew)
  height 40px
  transform skew(skew) translateX(14px)
  flex 3

walls()
  display flex
  height 50px
  width 140px
  
walls__front()
  height 50px
  flex 3
  
walls__side()
  height 50px
  flex 2
  
// Function for generating random numbers within a range  
random(min, max)
  return floor( math(0, "random") * max + min )  
  
// Light monochrome color scheme 
monotone(totalStops, degrees, hue, saturation, lightness, increment)
  stops = ()

  for i in 1..totalStops
    lightness += increment
    push(
      stops, 
      hsl(hue, saturation, lightness - increment) ((i - 1) / totalStops) * 100%,
      hsl(hue, saturation, lightness - increment) (i / totalStops) * 100%
    )

  linear-gradient(
    (degrees * 1deg),
    unquote( join( ',', stops ) )
  )  
  
// Light monochrome color scheme 
localcolor(totalStops, degrees, hue, saturation, lightness, increment)
  stops = ()

  for i in 1..totalStops
    hue += increment
    push(
      stops, 
      hsl(hue - increment, saturation, lightness) ((i - 1) / totalStops) * 100%,
      hsl(hue - increment, saturation, lightness) (i / totalStops) * 100%
    )

  linear-gradient(
    (degrees * 1deg),
    unquote( join( ',', stops ) )
  )  
  
