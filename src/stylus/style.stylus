  reflection_opacity: 0
}

body
  font-family roboto slab
  font-size 20
  font-weight 200
  line-height 1.5
  display flex
  justify-content flex-start
  align-items center
  height 100vh
  width 100vw
  flex-direction column


/**
 * This is the weatherMixin
 * @constructor
 * @param {object} weather_type - The weather object that you want to use.
 * @param {string} target_selector - The css class added to the body tag.
 * @param {number} reflection_opacity - The opacity of the haus reflected on the ground
 */

weatherMixin(weather_type, reflection_opacity = 0)
  #app{weather_type.selector}
    background white
    display flex
    flex-direction column
    position relative
    height 100vh
    width 100vw
    
    box-shadow inset 4rem 4rem 0  white
    
    &::before
      background-image: weather_type.sky-overlay
      background-size: weather_type.sky-overlay-size
      background-position: weather_type.sky-overlay-position
      mix-blend-mode: weather_type.sky-blendmode
      content: ''
      height: weather_type.sky-overlay-height
      width 100%
      position absolute
      z-index 2
  
    // Sky Gadients  
    .sky
      split()
      background-image: weather_type.sky
      background-size: weather_type.sky-size
      background-position: weather_type.sky-position

    .sky__haus
      haus()

    .sky__haus-roof
      roof()

    .sky__haus-roof-side
      roof__side(35deg)
      background: weather_type.roof-side

    .sky__haus-roof-front
      roof__front(-35deg)
      background: weather_type.roof-front

    .sky__haus-walls
      walls()

    .sky__haus-walls-front
      walls__front()
      background: weather_type.walls-front

    .sky__haus-walls-side
      walls__side()
      background: weather_type.roof-side

    // Ground Gradients
    .ground
      split(green, flex-start)
      background: weather_type.ground

    .ground__haus
      haus()
      opacity: weather_type.reflection_opacity

    .ground__haus-roof
      roof()
      order 2

    .ground__haus-roof-side
      roof__side(-35deg)
      background-color: weather_type.roof-side

    .ground__haus-roof-front
      roof__front(35deg)
      background-color: weather_type.roof-front

    .ground__haus-walls
      walls()
      order 1

    .ground__haus-walls-front
      walls__front()
      background-color: weather_type.walls-front

    .ground__haus-walls-side
      walls__side()
      background: weather_type.roof-side

.nav
  display flex
  align-items center
  justify-content space-between
  padding 0 4rem
  height 4.2rem
  width 100vw
  
  +breakpoint('sm')
    flex-direction column
    height 8rem
  
  
.logo
  //font-family Roboto Condensed
  font-size 2rem
  font-weight 700
  line-height 1
  position relative
  text-decoration none
  &:hover .free,
    //
  &:hover
    .code, .camp, .project
      transform translateX(0)
      
  +breakpoint('sm')
    width 100%
  
.free, .code, .camp, .project
  background-color white
  color indigo
  display inline-block
  overflow hidden
  transition all .33s cubic-bezier(.91,-0.44,.12,1.51)

.code
  transform translateX(-65%)
  
.camp
  transform translateX(-118%)
  
.project
  background-color white
  font-weight 200
  transform translateX(-79%)  
  
.controls
  display flex
  align-items center
  justify-content space-between
  
  span
    margin-left 1rem
    
.switch
  position relative
  display inline-block
  
  & > span
    position absolute
    top 5px
    pointer-events none
    font-weight bold
    text-transform uppercase
    text-shadow 0 1px 0 rgba(0, 0, 0, .06)
    width 50%
    text-align center
    
    &.on
      left 0
      padding-left 2px
      color #000
      
    &.off
      right 0
      padding-right 4px
      color #fff

input
  &.check-toggle-round-flat
    &:checked
      & ~ .off
        color #000
        
      & ~ .on
        color #fff
        
      & + label
        &:after
          margin-left 44px
          
    & + label
      padding 2px
      width 97px
      height 34px
      background-color #efab22
      border-radius 60px
      
      &:before
        top 2px
        left 2px
        bottom 2px
        right 2px
        background-color #efab22
        border-radius 60px
        
      &:after
        top 4px
        left 4px
        bottom 4px
        width 48px
        background-color #ffd455
        border-radius 52px
        transition margin 0.2s

.check-toggle
  position absolute
  margin-left -9999px
  visibility hidden
  
  & + label
    display block
    position relative
    cursor pointer
    outline none
    user-select none

input.check-toggle-round-flat + label:before, 
input.check-toggle-round-flat + label:after
  display block
  position absolute
  content ""
    
